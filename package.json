{
  "name": "nasa_openmct_cypress_tests",
  "version": "1.0.0",
  "description": "NASA OpenMCT Cypress automated tests",
  "main": "index.js",
  "scripts": {
    "test": "CYPRESS_ALLURE=true npx cypress run",
    "allure:report": "allure generate allure-results --clean -o allure-report",
    "allure:open": "allure open allure-report",
    "test:allure": "npm run test && npm run allure:report && npm run allure:open"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MiRGB/NASA_OpenMCT_Cypress_tests.git"
  },
  "keywords": [
    "cupress",
    "tests",
    "nasa"
  ],
  "author": "MiRGB",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MiRGB/NASA_OpenMCT_Cypress_tests/issues"
  },
  "homepage": "https://github.com/MiRGB/NASA_OpenMCT_Cypress_tests#readme",
  "devDependencies": {
    "allure-cypress": "^3.3.0",
    "cypress": "^14.4.1",
    "cypress-downloadfile": "^1.2.4"
  }
}
